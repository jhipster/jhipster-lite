{
  "name": "Java",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "17-bullseye",
      "INSTALL_MAVEN": "true",
      "MAVEN_VERSION": "3.8.5",
      "INSTALL_GRADLE": "false",
      "NODE_VERSION": "lts/*"
    }
  },
  "settings": {
    "java.home": "/docker-java-home",
    "maven.executable.path": "/usr/local/sdkman/candidates/maven/current/bin/mvn"
  },
  "extensions": ["vscjava.vscode-java-pack"],
  "forwardPorts": [7471],
  "remoteUser": "vscode"
}
